<ordsAlias>view_1</ordsAlias>
<userDefined>true</userDefined>
<userDefinedSQL><![CDATA[SELECT
    stores.customer.id_customer,
    stores.customer.customer_name,
    stores.managers.id_manager,
    stores.managers.manager_name,
    stores.orders.order_id,
    stores.orders.country,
    stores.orders.region,
    stores.orders.order_date,
    stores.orders.ship_date
FROM
         stores.customer
    INNER JOIN stores.orders ON stores.customer.id_customer = stores.orders.id_customer
    INNER JOIN stores.managers ON stores.managers.id_manager = stores.orders.id_manager]]></userDefinedSQL>
